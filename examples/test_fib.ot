// Test fibonacci implementation
function fib(n: number): number {
    if (n <= 1) {
        return n;
    }
    return fib(n - 1) + fib(n - 2);
}

function main(): void {
    let result: number = fib(10);
    console.log("fib(10) =", result);

    let i: number = 0;
    while (i <= 10) {
        console.log("fib(" + i + ") =", fib(i));
        i = i + 1;
    }
}

main();
