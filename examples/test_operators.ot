// Test all the new operators
console.log("Testing operators...");

// Arithmetic
let a: number = 10;
let b: number = 3;
console.log("10 * 3 =", a * b);
console.log("10 / 3 =", a / b);
console.log("10 % 3 =", a % b);

// Comparisons
console.log("5 <= 5:", 5 <= 5);
console.log("5 <= 6:", 5 <= 6);
console.log("5 >= 5:", 5 >= 5);
console.log("5 >= 4:", 5 >= 4);

// Loose equality
console.log("5 == '5':", 5 == "5");
console.log("5 === '5':", 5 === "5");
console.log("5 != '5':", 5 != "5");
console.log("5 !== '5':", 5 !== "5");

// Logical OR
console.log("true || false:", true || false);
console.log("false || false:", false || false);
console.log("true || true:", true || true);
