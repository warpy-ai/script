// Minimal test for object method calls
console.log("Testing object method calls...");

// Define a simple function
function greet(name) {
    return "Hello, " + name;
}

// Store it in an object
let obj = {
    sayHello: greet,
    getValue: function() { return 42; }
};

// Test 1: Call global function directly
console.log("Test 1 - Direct call: " + greet("World"));

// Test 2: Access the function property
console.log("Test 2 - Property type: " + typeof obj.sayHello);

// Test 3: Call method on object
console.log("Test 3 - Method call: " + obj.sayHello("Test"));

console.log("All tests complete!");
